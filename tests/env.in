TEST_SHELL=${TEST_SHELL-@TEST_SHELL@}
EGREP="@EGREP@"
AWK="@AWK@"

# pass configuration to regression tests
abs_top_srcdir=@abs_top_srcdir@
sysconfdir=@sysconfdir@
EXEEXT=@EXEEXT@
TEST_SSH_IPV6=@TEST_SSH_IPV6@
TEST_SSH_UTF8=@TEST_SSH_UTF8@
USE_OPENSSL_STORE2=@USE_OPENSSL_STORE2@

# OCSP enabled
@OCSP_ON@SSH_OCSP_ENABLED=:
@OCSP_OFF@SSH_OCSP_ENABLED=false

# LDAP environment:
@LDAP_ON@SSH_LDAP_ENABLED=:
@LDAP_OFF@SSH_LDAP_ENABLED=false

@LDAP_ON@LDAP_SYSCONFDIR="@LDAP_SYSCONFDIR@"
@LDAP_ON@LDAP_LIBEXECDIR="@LDAP_LIBEXECDIR@"
@LDAP_ON@LDAP_BINDIR="@LDAP_BINDIR@"
@LDAP_ON@LDAPADD="@LDAPADD@"

# FIPS capable openssl
OPENSSL_FIPS_MODE=@OPENSSL_FIPS_MODE@
